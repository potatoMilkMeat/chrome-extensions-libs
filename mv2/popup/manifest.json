{ 
  "manifest_version": 2,
  "name": "clong-1 popup",
  "version": "0.0.1",
  "description": "扩展-程龙",
  
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon-128.png",
    "default_title": "这是一个示例Chrome插件",
    "default_popup": "popup.html"
  }
}
